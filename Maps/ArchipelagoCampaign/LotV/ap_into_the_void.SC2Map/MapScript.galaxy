    libVCMI_gf_PlaySoundCinematic(SoundLink("Hydralisk_Ready", -1), PlayerGroupAll(), 200.0, 0.0);
    Wait(3.0, c_timeGame);
    libVCMI_gf_PlaySoundCinematic(SoundLink("AP_ZerglingSCBW@Pissed2", -1), PlayerGroupAll(), 200.0, 0.0);
    Wait(3.0, c_timeGame);
    CameraApplyInfo(gv_pLAYER_01_USER, CameraInfoFromId(1128062905), 3.0, -1, 10, true);
    Wait(3.0, c_timeGame);
    libNtve_gf_SendActorMessageToUnit(lv_ultra, "AnimPlay Attack Birth NonLooping -1.000000 -1.000000 2.000000 AsDuration");
    libVCMI_gf_PlaySoundCinematic(SoundLink("AP_SCBW_Ultralisk_Ready", -1), PlayerGroupAll(), 150.0, 0.0);
    Wait(2.0, c_timeGame);
    UnitSetPropertyFixed(lv_ultra, c_unitPropMovementSpeed, 1.5);
    UnitIssueOrder(lv_ultra, OrderTargetingPoint(AbilityCommand("move", 0), PointFromId(439)), c_orderQueueReplace);
    Wait(3.0, c_timeGame);
    UnitRemove(lv_ultra);
    CameraApplyInfo(gv_pLAYER_01_USER, CameraInfoFromId(442198874), 0.0, -1, 10, true);
    CameraApplyInfo(gv_pLAYER_01_USER, CameraInfoFromId(1940264566), 12.0, -1, 10, true);
